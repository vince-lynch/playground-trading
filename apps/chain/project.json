{
  "root": "apps/chain",
  "sourceRoot": "apps/chain/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/chain",
        "main": "apps/chain/src/main.js",
        "tsConfig": "apps/chain/tsconfig.app.json",
        "assets": [
          "apps/chain/src/assets"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/chain/src/environments/environment.js",
              "with": "apps/chain/src/environments/environment.prod.js"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "docker-compose up ganache-cli"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/chain/**/*.js"
        ]
      }
    }
  },
  "tags": []
}
